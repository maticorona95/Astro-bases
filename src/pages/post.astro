---
const response = await fetch ('https://jsonplaceholder.typicode.com/posts')
const post = await response.json ()

---

<h1>Mis Publicaciones</h1>

{
    post.map(post => (
        <div>
            <h5 class="title">{post.userId}</h5>
            <h3>{post.title}</h3>
            <p style="color:blue">{post.body}</p>
        </div>
    ))
}

<style>
    .title {
        color: chocolate;
    }
</style>